.clients-slider
  background: #F7F7F7
  padding 9rem 0
  &__container
    width 100%
    height: 100%
  &__slide
    text-align: center
    display flex
    justify-content: center
    align-items: center
  &__img
    height: 10rem
    object-fit: contain

.clients-slider .swiper-button-prev, .clients-slider .swiper-button-next
  background: none
